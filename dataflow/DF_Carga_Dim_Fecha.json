{
	"name": "DF_Carga_Dim_Fecha",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Source_DetalleVentas",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Sink_Dim_Fecha",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "orden"
				},
				{
					"name": "envio"
				},
				{
					"name": "union1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sls_ord_num as string,",
				"          sls_prd_key as string,",
				"          sls_cust_id as string,",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string,",
				"          sls_sales as string,",
				"          sls_quantity as string,",
				"          sls_price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(Fecha_Base = trim(sls_order_dt)) ~> orden",
				"source1 derive(Fecha_Base = trim(sls_ship_dt)) ~> envio",
				"orden, envio union(byName: false)~> union1",
				"aggregate1 derive(Crear_FechaKey = toInteger(trim(Fecha_Base))) ~> derivedColumn1",
				"union1 aggregate(groupBy(Fecha_Base),",
				"     Conteo_Temporal = count(1)) ~> aggregate1",
				"derivedColumn1 select(mapColumn(",
				"          {FechaCompleta     } = Fecha_Base,",
				"          FechaKey = Crear_FechaKey",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          FechaKey as integer,",
				"          FechaCompleta as date,",
				"          Anio as integer,",
				"          Mes as integer,",
				"          Dia as integer,",
				"          Trimestre as integer,",
				"          DiaDeLaSemana as string,",
				"          EsFinDeSemana as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}